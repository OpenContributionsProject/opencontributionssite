{
	"dependencies": {
		"@next/font": "13.0.7",
		"next": "13.0.7",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"description": "A resource to help advocate for and explain corporate contributions to open source.",
	"devDependencies": {
		"@types/node": "18.11.15",
		"@types/react-dom": "18.0.9",
		"@types/react": "18.0.26",
		"@typescript-eslint/eslint-plugin": "^5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"@vitest/coverage-c8": "^0.26.2",
		"cspell": "^6.12.0",
		"eslint-config-next": "13.0.7",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-deprecation": "^1.3.3",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-jsonc": "^2.5.0",
		"eslint-plugin-markdown": "^3.0.0",
		"eslint-plugin-no-only-tests": "^3.1.0",
		"eslint-plugin-regexp": "^1.11.0",
		"eslint-plugin-simple-import-sort": "^8.0.0",
		"eslint-plugin-typescript-sort-keys": "^2.1.0",
		"eslint-plugin-vitest": "^0.0.20",
		"eslint": "^8.29.0",
		"husky": "^8.0.2",
		"jsonc-eslint-parser": "^2.1.0",
		"lint-staged": "^13.1.0",
		"markdownlint-cli": "^0.32.2",
		"npm-package-json-lint-config-default": "^5.0.0",
		"npm-package-json-lint": "^6.3.0",
		"pnpm": "^7.18.2",
		"pnpm-deduplicate": "^0.4.0",
		"prettier": "^2.8.1",
		"sentences-per-line": "^0.2.1",
		"ts-prune": "^0.10.3",
		"typescript": "^4.9.4",
		"vitest": "^0.25.0"
	},
	"engines": {
		"node": ">=18"
	},
	"license": "MIT",
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"name": "site",
	"packageManager": "pnpm@7.18.2",
	"repository": {
		"type": "git",
		"url": "https://github.com/OpenContributionsProject/opencontributionssite"
	},
	"scripts": {
		"build": "next build",
		"format": "prettier \"**/*\" --ignore-unknown",
		"format:write": "pnpm format --write",
		"dev": "next dev",
		"lint": "next lint --max-warnings 0 --report-unused-disable-directives error",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:package": "npmPkgJsonLint .",
		"lint:packages": "pnpm-deduplicate --list",
		"lint:prune": "ts-prune",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"prepare": "husky install",
		"start": "next start",
		"tsc": "tsc",
		"test": "vitest"
	},
	"version": "0.1.0"
}
