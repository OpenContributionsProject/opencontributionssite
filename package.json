{
	"dependencies": {
		"@mdx-js/loader": "^2.2.1",
		"@mdx-js/react": "^2.2.1",
		"@next/font": "13.1.4",
		"@next/mdx": "^13.1.4",
		"next": "13.1.4",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-slug": "^5.1.0",
		"remark-gfm": "^3.0.1"
	},
	"description": "A resource to help advocate for and explain corporate contributions to open source.",
	"devDependencies": {
		"@types/node": "18.11.18",
		"@types/react": "18.0.27",
		"@types/react-dom": "18.0.10",
		"@typescript-eslint/eslint-plugin": "^5.48.2",
		"@typescript-eslint/parser": "^5.48.2",
		"@vitest/coverage-c8": "^0.27.2",
		"cspell": "^6.19.2",
		"eslint": "^8.32.0",
		"eslint-config-next": "13.1.4",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-deprecation": "^1.3.3",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-jsonc": "^2.6.0",
		"eslint-plugin-markdown": "^3.0.0",
		"eslint-plugin-no-only-tests": "^3.1.0",
		"eslint-plugin-regexp": "^1.12.0",
		"eslint-plugin-simple-import-sort": "^9.0.0",
		"eslint-plugin-typescript-sort-keys": "^2.1.0",
		"eslint-plugin-vitest": "^0.0.30",
		"husky": "^8.0.3",
		"jsonc-eslint-parser": "^2.1.0",
		"lint-staged": "^13.1.0",
		"markdownlint-cli": "^0.33.0",
		"npm-package-json-lint": "^6.4.0",
		"npm-package-json-lint-config-default": "^5.0.0",
		"pnpm": "^7.25.1",
		"pnpm-deduplicate": "^0.4.1",
		"prettier": "^2.8.3",
		"sentences-per-line": "^0.2.1",
		"ts-prune": "^0.10.3",
		"typescript": "^4.9.4",
		"vitest": "^0.27.2"
	},
	"engines": {
		"node": ">=18"
	},
	"license": "MIT",
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"name": "site",
	"packageManager": "pnpm@7.25.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/OpenContributionsProject/opencontributionssite"
	},
	"scripts": {
		"build": "next build",
		"format": "prettier \"**/*\" --ignore-unknown",
		"format:write": "pnpm format --write",
		"dev": "next dev",
		"lint": "next lint --max-warnings 0 --report-unused-disable-directives error",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:package": "npmPkgJsonLint .",
		"lint:packages": "pnpm-deduplicate --list",
		"lint:prune": "ts-prune",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"prepare": "husky install",
		"start": "next start",
		"tsc": "tsc",
		"test": "vitest"
	},
	"version": "0.1.0"
}
